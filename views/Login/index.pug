  body
    .section     
      .center
        form#formlogin.container
          .z-depth-1.row(style='display: inline-block; padding: 32px 48px 0px 48px; border: 1px solid #EEE;')
            .row
              .input-field.col.s12
                input(type='text', name='user', autocomplete='off')                             
                label(for='email') Usuario
            .row
              .input-field.col.s12
                input(type='password', name='pass')                         
                label(for='password') Contrase&ntilde;a                        
            br               
            .row
              button#login.col.s12.btn.btn-large.waves-effect.darken-4(type='button' style="background-color:#6B2A6C") Login
